   "cell_type": "code",
   "execution_count": 29,
   "id": "dc184b98-b112-481e-b6c2-5cc394dcd760",
   "metadata": {},
   "outputs": [],
   "source": [
    "model = Sequential()\n",
    "# And then we add a few layers to it\n",
    "# Encoding\n",
    "model.add(Dense(128, activation = 'relu', input_dim = 784))\n",
    "model.add(Dense(64, activation = 'relu'))\n",
    "# Decoding\n",
    "model.add(Dense(128, activation = 'relu'))\n",
    "model.add(Dense(784, activation = 'sigmoid'))"
   ]
  },
